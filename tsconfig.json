{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "node",
		"target": "esnext",
		"jsx": "preserve",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["src/App/*"]
		},
		"lib": [
			"dom",
			"esnext"
		]
	}
}
